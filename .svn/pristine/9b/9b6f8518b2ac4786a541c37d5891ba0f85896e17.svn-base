package pers.guanzf.common.core.service;

import java.util.List;

import pers.guanzf.common.core.model.Produce;

public interface ProduceService {

	/**
	 * @return 全部商品信息
	 */
	public List<Produce> listProduce();
	
	/**
	 * @param produceId 主键
	 * @return 具体商品详细信息
	 */
	public Produce getProduceInfo(String produceId);
	
	/**
	 * @param produceId 主键
	 * @return 根据主键删除商品信息
	 */
	public int deleteProduceInfo(String produceId);
	
	/**
	 * 插入产品信息
	 */
	public int insertProduceInfo(Produce produce);
	
	public int updateProduceInfo(Produce produce);
	
}
